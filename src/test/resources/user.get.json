{
  "method":"user.get",
  "title":"get single user",
  "request":{
    "id":{
      "$schema":"http://json-schema.org/draft-04/schema#",
      "title":"String",
      "type":"string"
    }
  },
  "response":{
    "$schema":"http://json-schema.org/draft-04/schema#",
    "title":"Sample User",
    "type":"object",
    "additionalProperties":false,
    "properties":{
      "name":{
        "type":"string"
      },
      "inner":{
        "$ref":"#/definitions/InnerObj"
      },
      "interface":{
        "oneOf":[
          {
            "$ref":"#/definitions/SampleImpl1"
          },
          {
            "$ref":"#/definitions/SampleImpl2"
          }
        ]
      }
    },
    "definitions":{
      "InnerObj":{
        "type":"object",
        "additionalProperties":false,
        "properties":{
          "age":{
            "type":"integer",
            "minimum":0,
            "maximum":200
          },
          "string":{
            "type":"string"
          },
          "userType":{
            "type":"string",
            "enum":[
              "A1",
              "B1",
              "C1"
            ]
          }
        },
        "required":[
          "age"
        ]
      },
      "SampleImpl1":{
        "type":"object",
        "additionalProperties":false,
        "properties":{
          "name":{
            "type":"string",
            "enum":[
              "1"
            ],
            "default":"1"
          },
          "n":{
            "type":"integer",
            "maximum":10
          }
        },
        "title":"1",
        "required":[
          "name",
          "n"
        ]
      },
      "SampleImpl2":{
        "type":"object",
        "additionalProperties":false,
        "properties":{
          "name":{
            "type":"string",
            "enum":[
              "2"
            ],
            "default":"2"
          },
          "v":{
            "type":"number"
          }
        },
        "title":"2",
        "required":[
          "name",
          "v"
        ]
      }
    }
  }
}